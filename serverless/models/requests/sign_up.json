{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Sign Up Request Schema",
  "required": [
    "userName", 
    "password",
    "email"
  ],
  "properties": {
    "userName": {
      "type": "string",
      "title": "User Name Schema",
      "pattern": "^[a-zA-Z0-9-'_ ]+$"
    },
    "password": {
      "type": "string",
      "title": "Password Schema"
    },
    "email": {
      "type": "string",
      "title": "Email Schema"
    }
  }
}
