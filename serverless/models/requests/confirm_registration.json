{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Confirm Registration Request Schema",
  "required": ["userName", "verificationCode"],
  "properties": {
    "userName": {
      "type": "string",
      "title": "User Name Schema",
      "pattern": "^[a-zA-Z0-9-'_ ]+$"
    },
    "verificationCode": {
      "type": "number",
      "title": "Verification Code Schema"
    }
  }
}
