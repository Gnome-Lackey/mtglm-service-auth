{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Login Request Schema",
  "required": ["userName", "password"],
  "properties": {
    "userName": {
      "type": "string",
      "title": "User Name Schema",
      "pattern": "^[a-zA-Z0-9-'_ ]+$"
    },
    "password": {
      "type": "string",
      "title": "Password Schema"
    }
  }
}
